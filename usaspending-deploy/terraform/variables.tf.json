{
  "variable": {
    "aws_region": {
      "description": "The AWS region to create things in.",
      "default": "us-gov-west-1"
    },

    "aws_amis": {
      "default": {
        "us-gov-west-1": "ami-c0d055a1"
      }
    },

    "availability_zones": {
      "default": "us-gov-west-1a,us-gov-west-1b",
      "description": "List of availability zones"
    },

    "key_name": {
      "default": "DA-SSHKey-Staging",
      "description": "Keys to access instance"
    },

    "api_instance_type": {
      "default": "m3.medium",
      "description": "AWS instance type"
    },

    "api_asg_min": {
      "description": "Min numbers of servers in ASG",
      "default": "1"
    },

    "api_asg_max": {
      "description": "Max numbers of servers in ASG",
      "default": "5"
    },

    "api_asg_desired": {
      "description": "Desired numbers of servers in ASG",
      "default": "1"
    },

    "subnets": {
      "default": "subnet-5e0e8e29,subnet-dce17bb9",
      "description": "List of subnets within VPC"
    },

    "api_sec_groups": {
      "description": "Security Groups JenkinsNode, RemoteAccess, ELB-API",
      "default": "sg-c99e18ac,sg-f815be9d,sg-b915bedc"
    },

    "api_elb": {
      "description": "Name of API Load Balancer",
      "default": "usaspending-api-stg-elb"
    },

    "api_iam_profile": {
      "description": "IAM Profile",
      "default": "DA-Dev-EC2-dsapi"
    },

    "api_name_prefix": {
      "description": "Prefix for API ASG and LC names",
      "default": "USASpending-API-Stg"
    }
    
  }
}